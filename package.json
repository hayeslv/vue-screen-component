    "@types/ramda": "^0.28.4",
    "@types/video.js": "^7.3.32",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "bumpp": "^7.1.1",
    "echarts": "^5.3.1",
    "eslint": "^8.11.0",
    "ramda": "^0.28.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.3",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
